## specify graphical parameters
export(metaPar)
export(metaParDefaults)
export(resetDefaults)

## print
export(print.metabinDF)
export(print.metaPar)

## make hetero stats descriptions
export(makeStatDesc)
export(makeLabelDesc)

## convenience functions
